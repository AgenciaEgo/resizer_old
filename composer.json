{
    "name": "agenciaego/resizer",
    "description": ":description",
    "license": "MIT",
    "authors": [
        {
            "name": "Eric Kersten",
            "email": "eric.kersten@egoargentina.com",
            "homepage": "http://agenciaego.com.ar"
        }
    ],
    "homepage": "https://github.com/AgenciaEgo/resizer",
    "keywords": ["Laravel", "Resizer"],
    "require": {
        "illuminate/support": "~5",
        "intervention/image": "^2.4",
        "intervention/imagecache": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "AgenciaEgo\\Resizer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AgenciaEgo\\Resizer\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AgenciaEgo\\Resizer\\ResizerServiceProvider"
            ],
            "aliases": {
                "Resizer": "AgenciaEgo\\Resizer\\Facades\\Resizer"
            }
        }
    }
}